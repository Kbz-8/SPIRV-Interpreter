 [nzsl_version("1.1")]
 [feature(float64)]
 module;
 
 struct FragOut
 {
     [location(0)] color: vec4[f32]
 }

 [entry(frag)]
 fn main() -> FragOut
 {
     let op1: f64 = 0.0;
     let op2: f64 = 9.0;
     let color: f32;
     if (op1 == op2)
         color = f32(op1);
     else
         color = f32(op2);

     let output: FragOut;
     output.color = vec4[f32](color, color, color, color);
     return output;
 }
