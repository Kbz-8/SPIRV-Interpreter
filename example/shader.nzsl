[nzsl_version("1.1")]
[feature(float64)]
module;

struct FragOut
{
    [location(0)] color: vec4[i32]
}

fn fibonacci(n: i32) -> i32
{
    if (n <= i32(1)) return n;
    return fibonacci(n - i32(1)) + fibonacci(n - i32(2));
}

[entry(frag)]
fn main() -> FragOut
{
    let output: FragOut;
    output.color = vec4[i32](fibonacci(2), fibonacci(2), fibonacci(2), fibonacci(2));
    return output;
}
