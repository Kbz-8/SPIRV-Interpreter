[nzsl_version("1.1")]
[feature(float64)]
module;

struct FragOut
{
    [location(0)] color: vec4[f32]
}

fn computeColor() -> f32
{
	return 1.0;
}

[entry(frag)]
fn main() -> FragOut
{
    let output: FragOut;
    output.color = vec4[f32](computeColor(), computeColor(), computeColor(), computeColor());
    return output;
}
