[nzsl_version("1.1")]
[feature(float64)]
module;

struct FragOut
{
    [location(0)] color: vec4[f32]
}

[entry(frag)]
fn main() -> FragOut
{
    let value: f32 = 0.0;
    for i in 1 -> 20 {
        value *= f32(i);
    }
    let output: FragOut;
    output.color = vec4[f32](value, value, value, value);
    return output;
}
